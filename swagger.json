      "addPetRequest": {
        "type": "object",
        "required": [
          "name",
          "birthday",
          "type",
          "comments",
          "location",
          "price",
          "filter"
        ],
        "properties": {
          "name": {
            "type": "string",
            "description": "Pet's name",
            "format": "name",
            "example": "Barsik"
          },
          "birthday": {
            "type": "number",
            "description": "Pet's birthday",
            "format": "number",
            "example": "11.12.2020"
          },
          "type": {
            "type": "string",
            "description": "Pet's type",
            "example": "Sheepdog"
          },
          "comments": {
            "type": "string",
            "description": "Comments about your pet ",
            "example": "Jack is a handsome Basenji with short, shiny red fur and perky ears. He's an active and intelligent dog that loves to explore and play. Jack is independent and strong-willed, but also affectionate and loyal to his family. He's a curious and energetic companion that brings joy and adventure to his family's life."
          },
          "location": {
            "type": "string",
            "description": "Pet's location",
            "format": "string",
            "example": "Lviv"
          },
          "price": {
            "type": "number",
            "description": "Pet's price",
            "format": "number",
            "example": 21312
          },
          "filter": {
            "type": "string",
            "description": "sell, lost/found, in good hands, found",
            "format": "string",
            "example": "sell"
          }
        }
      },
