{
  "openapi": "3.0.1",
  "info": {
    "version": "0.1.15",
    "title": "Swagger for pets",
    "description": "Showing off swagger-ui-express",
    "license": {
      "name": "MIT",
      "url": "https://opensource.org/licenses/MIT"
    }
  },
  "consumes": ["application/json"],
  "produces": ["application/json"],
  "servers": [
    { "url": "https://pet-web-server.onrender.com" },
    { "url": "http://localhost:3030/" }
  ],
  "tags": [
    {
      "name": "Auth",
      "description": "Authorization endpoints"
    },
    {
      "name": "User",
      "description": "User endpoints"
    },
    {
      "name": "Other",
      "description": "Public routes like news and sponsors"
    },
    {
      "name": "Notices",
      "description": "Public routes with notices and info about sell, find, and so on"
    }
  ],
  "paths": {
    "/api/auth/registration": {
      "post": {
        "tags": ["Auth"],
        "summary": "User registration",
        "parameters": [],
        "requestBody": {
          "description": "Registration's object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/RegistrationRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/RegistrationResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request (invalid request body)",
            "content": {}
          },
          "409": {
            "description": "Provided email already exists",
            "content": {}
          }
        }
      }
    },
    "/api/auth/login": {
      "post": {
        "tags": ["Auth"],
        "summary": "User login",
        "parameters": [],
        "requestBody": {
          "description": "Login's object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/LoginResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request (invalid request body)",
            "content": {}
          },
          "409": {
            "description": "No Authorization",
            "content": {}
          }
        }
      }
    },
    "/api/auth/logout": {
      "post": {
        "tags": ["Auth"],
        "summary": "User logout",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "description": "id object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/logoutRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful logout",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/logoutResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request ",
            "content": {}
          }
        }
      }
    },
    "/api/auth/verify/:verificationToken": {
      "get": {
        "tags": ["Auth"],
        "summary": "Verification User",
        "parameters": [
          {
            "in": "path",
            "name": "verificationToken",
            "required": true,
            "type": "string",
            "description": "verificationToken User's"
          }
        ],

        "responses": {
          "200": {
            "description": "Email verify success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/verifyResponse"
                }
              }
            }
          },
          "401": {
            "description": "Email not found ",
            "content": {}
          }
        }
      }
    },
    "/api/auth/verify": {
      "post": {
        "tags": ["Auth"],
        "summary": "resend verify email user",
        "parameters": [],

        "requestBody": {
          "description": "id object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/resendVerifyRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Verify email send success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/resendVerifyResponse"
                }
              }
            }
          },
          "400": {
            "description": "Email not found ",
            "content": {}
          },
          "401": {
            "description": "Email already verify ",
            "content": {}
          }
        }
      }
    },
    "/api/user/pets": {
      "get": {
        "tags": ["User"],
        "summary": "List your pets",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "responses": {
          "200": {
            "description": "Successful list pets",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/listPetResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request ",
            "content": {}
          }
        }
      },
      "post": {
        "tags": ["User"],
        "summary": "Add your pet",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "description": "pet object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/addPetRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful added pet",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/addPetResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request ",
            "content": {}
          }
        }
      }
    },
    "api/user/pets/:id": {
      "put": {
        "tags": ["User"],
        "summary": "update info about my pet",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "type": "string",
            "description": "Pet's id"
          }
        ],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "description": "pet object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/updateInfoPetRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful update info operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/updateInfoPetResponses"
                }
              }
            }
          },
          "400": {
            "description": "Bad request (invalid request body)",
            "content": {}
          }
        }
      },
      "delete": {
        "tags": ["User"],
        "summary": "delete pet by id",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "type": "string",
            "description": "Pet's id"
          }
        ],
        "security": [{ "Bearer": [] }],
        "responses": {
          "204": {
            "description": "Successful delete ",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/deletePetResponses"
                }
              }
            }
          },
          "400": {
            "description": "Bad request (invalid id)",
            "content": {}
          }
        }
      }
    },
    "api/user/current": {
      "get": {
        "tags": ["User"],
        "summary": "Get info current user",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/infoCurrentUserResponses"
                }
              }
            }
          },
          "401": {
            "description": "Not authorized",
            "content": {}
          }
        }
      },
      "put": {
        "tags": ["User"],
        "summary": "Update info current user",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "requestBody": {
          "description": "user object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/updateInfoUserRequest"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Successful update info ",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/updateInfoUserResponses"
                }
              }
            }
          },
          "400": {
            "description": "Bad request",
            "content": {}
          }
        }
      }
    },
    "api/user/avatar": {
      "patch": {
        "tags": ["User"],
        "summary": "Update avatar user",
        "parameters": [],
        "security": [{ "Bearer": [] }],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/"
                }
              }
            }
          },
          "401": {
            "description": "Not authorized",
            "content": {}
          }
        }
      }
    },
    "/api/news?offset=3&limit=25": {
      "get": {
        "tags": ["Other"],
        "summary": "Return list of news about pets",
        "parameters": [
          {
            "in": "query",
            "name": "offset",
            "schema": {
              "type": "integer"
            },
            "description": "The page number",
            "requred": false,
            "type": "number",
            "example": 5,
            "default": 1
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer"
            },
            "description": "The number of items",
            "requred": false,
            "type": "number",
            "example": 20,
            "default": 10
          }
        ],
        "responses": {
          "200": {
            "description": "Successful fetch news",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/newsResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/notices": {
      "get": {
        "tags": ["Notices"],
        "summary": "Get all notices"
      },
      "post": {
        "tags": ["Notices"],
        "summary": "add new notice"
      }
    },
    "/api/notices/{noticeId}": {
      "get": {
        "tags": ["Notices"],
        "summary": "Get info about one notice"
      },
      "delete": {
        "tags": ["Notices"],
        "summary": "Get info about one notice"
      }
    },
    "/api/notices/{noticeId}/addToFavorite": {
      "patch": {
        "tags": ["Notices"],
        "summary": "Add notice to Favorite"
      }
    },
    "/api/notices/{noticeId}/removeFromFavorite": {
      "patch": {
        "tags": ["Notices"],
        "summary": "Remove notice to Favorite"
      }
    }
  },

  "components": {
    "schemas": {
      "RegistrationRequest": {
        "type": "object",
        "required": ["name", "email", "password"],
        "properties": {
          "name": {
            "type": "string",
            "description": "User's name",
            "format": "email",
            "example": "Godrik"
          },
          "email": {
            "type": "string",
            "description": "User's email",
            "format": "email",
            "example": "godrik999@gmail.com"
          },
          "password": {
            "type": "string",
            "description": "User's password",
            "example": "qwerty123"
          }
        }
      },
      "RegistrationResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "User's id"
          },
          "email": {
            "type": "string",
            "description": "User's email",
            "format": "email"
          }
        },
        "example": [
          {
            "email": "godrik999@gmail.com",
            "id": "8976asdf897asd02798hc73jnh"
          }
        ]
      },
      "LoginRequest": {
        "type": "object",
        "required": ["email", "password"],
        "properties": {
