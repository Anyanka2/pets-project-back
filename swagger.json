    "/api/news?offset=3&limit=25": {
      "get": {
        "tags": [
          "Other"
        ],
        "summary": "Return list of news about pets",
        "parameters": [
          {
            "in": "query",
            "name": "offset",
            "schema": {
              "type": "integer"
            },
            "description": "The page number",
            "requred": false,
            "type": "number",
            "example": 5,
            "default": 1
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer"
            },
            "description": "The number of items",
            "requred": false,
            "type": "number",
            "example": 20,
            "default": 10
          }
        ],
        "responses": {
          "200": {
            "description": "Successful fetch news",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/newsResponse"
                }
              }
            }
