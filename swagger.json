    "/api/auth/verify/:verificationToken": {
      "get": {
        "tags": [
          "Auth"
        ],
        "summary": "Verification User",
        "parameters": [
          {
            "in": "path",
            "name": "verificationToken",
            "required": true,
            "type": "string",
            "description": "verificationToken User's"
          }
        ],
        "responses": {
          "200": {
            "description": "Email verify success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/verifyResponse"
                }
              }
            }
          },
          "400": {
            "description": "Email not found ",
            "content": {}
          },
          "401": {
            "description": "Email already verify ",
            "content": {}
          }
        }
      }
    },
    "/api/auth/verify": {
      "post": {
        "tags": [
          "Auth"
        ],
        "summary": "resend verify email user",
        "parameters": [],
        "requestBody": {
