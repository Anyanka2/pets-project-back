        "responses": {
          "200": {
            "description": "Successful update info operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/updateInfoPetResponses"
                }
              }
            }
          },
          "400": {
            "description": "Bad request (invalid request body)",
            "content": {}
          }
        }
      },
      "delete": {
        "tags": [
          "User"
        ],
        "summary": "delete pet by id",
        "parameters": [
          {
            "in": "path",
            "name": "id",
            "required": true,
            "type": "string",
            "description": "Pet's id"
          }
        ],
        "security": [
          {
            "Bearer": []
          }
        ],
        "responses": {
          "204": {
            "description": "Successful delete ",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/deletePetResponses"
                }
              }
            }
          },
          "400": {
            "description": "Bad request (invalid id)",
            "content": {}
          }
        }
