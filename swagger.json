          }
        }
      }
    },
    "/api/news?offset=3&limit=25": {
      "get": {
        "tags": [
          "Other"
        ],
        "summary": "Return list of news about pets",
        "parameters": [
          {
            "in": "query",
            "name": "offset",
            "schema": {
              "type": "integer"
            },
            "description": "The page number",
            "requred": false,
            "type": "number",
            "example": 5,
            "default": 1
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "integer"
            },
            "description": "The number of items",
            "requred": false,
            "type": "number",
            "example": 20,
            "default": 10
          }
        ],
        "responses": {
          "200": {
            "description": "Successful fetch news",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/newsResponse"
                }
              }
            }
          }
        }
      }
    },
    "/api/notices": {
      "get": {
        "tags": [
          "Notices"
        ],
        "summary": "Get notices",
        "parameters": [
          {
            "in": "query",
            "name": "keyword",
            "schema": {
              "type": "string"
            },
            "description": "Part of title",
            "requred": false,
            "type": "string",
            "example": "title1"
          },
          {
            "in": "query",
            "name": "category",
            "schema": {
              "type": "string"
            },
            "description": "Notice category",
            "requred": false,
            "type": "string",
            "example": "sell"
          },
          {
            "in": "query",
            "name": "sex",
            "schema": {
              "type": "string"
            },
            "description": "Sex of a pet",
            "requred": false,
            "type": "string",
            "example": "male"
          },
          {
            "in": "query",
            "name": "gt",
            "schema": {
              "type": "number"
            },
            "description": "Pets that olded then {number}",
            "requred": false,
            "type": "number",
            "example": 2
          },
          {
            "in": "query",
            "name": "lt",
            "schema": {
              "type": "number"
            },
            "description": "Pets that younger then {number}",
            "requred": false,
            "type": "number",
            "example": 2
          },
          {
            "in": "query",
            "name": "favorite",
            "schema": {
              "type": "boolean"
            },
            "description": "Favorite notices of the user",
            "requred": false,
            "type": "boolean",
            "example": true
          },
          {
            "in": "query",
            "name": "own",
            "schema": {
              "type": "boolean"
            },
            "description": "Own notices of the user",
            "requred": false,
            "type": "boolean",
            "example": true
          },
          {
            "in": "query",
            "name": "page",
            "schema": {
              "type": "number"
            },
            "description": "Pagination page",
            "requred": false,
            "type": "number",
            "example": 1,
            "default": 1
          },
          {
            "in": "query",
            "name": "limit",
            "schema": {
              "type": "number"
            },
            "description": "Items on page",
            "requred": false,
            "type": "number",
            "example": 4,
            "default": 12
          }
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/noticesResponse"
                }
              }
            }
          },
          "401": {
            "description": "Not authorized",
            "content": {}
          }
        }
      },
      "post": {
        "tags": [
          "Notices"
        ],
        "summary": "Add new notice",
        "security": [
          {
            "Bearer": []
          }
        ],
        "parameters": [
          {
            "in": "query",
            "name": "title",
            "schema": {
              "type": "string"
            },
            "description": "Title of the notice",
            "requred": true,
            "type": "string",
            "example": "title1"
          },
          {
            "in": "query",
            "name": "category",
            "schema": {
              "type": "string"
            },
            "description": "Notice category",
            "requred": true,
            "type": "string",
            "example": "on good hands"
          },
          {
            "in": "query",
            "name": "sex",
            "schema": {
              "type": "string"
            },
            "description": "Sex of a pet",
            "requred": true,
            "type": "string",
            "example": "male"
          },
          {
            "in": "query",
            "name": "name",
            "schema": {
              "type": "string"
            },
            "description": "Pet's name",
            "requred": true,
            "type": "string",
            "example": "Valera"
          },
          {
            "in": "query",
            "name": "birthday",
            "schema": {
              "type": "number"
            },
            "description": "Pet's birthday",
            "requred": true,
            "type": "number",
            "format": "number",
            "example": "11.12.2020"
          },
          {
            "in": "query",
            "name": "type",
            "schema": {
              "type": "string"
            },
            "description": "Pet's type",
            "requred": true,
            "type": "string",
            "example": "Sheepdog"
          },
          {
            "in": "query",
            "name": "comments",
            "schema": {
              "type": "string"
            },
            "description": "Comments about your pet ",
            "requred": true,
            "type": "string",
            "example": "Jack is a handsome Basenji with short, shiny red fur and perky ears. He's an active and intelligent dog that loves to explore and play. Jack is independent and strong-willed, but also affectionate and loyal to his family. He's a curious and energetic companion that brings joy and adventure to his family's life."
          },
          {
            "in": "query",
            "name": "location",
            "schema": {
              "type": "string"
            },
            "description": "Pet's location",
            "type": "string",
            "example": "Lviv"
          },
          {
            "in": "query",
            "name": "price",
            "schema": {
              "type": "number"
            },
            "description": "Pet's price",
            "type": "number",
            "example": 21312
          }
        ],
        "responses": {
          "200": {
            "description": "Notice added",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/noticesResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request ",
            "content": {}
          }
        }
      }
    },
    "/api/notices/{noticeId}": {
      "get": {
        "tags": [
