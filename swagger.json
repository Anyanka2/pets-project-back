            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/resendVerifyResponse"
                }
              }
            }
          },
          "400": {
            "description": "Email not found ",
            "content": {}
          },
          "401": {
            "description": "Email already verify ",
            "content": {}
          }
        }
      }
    },
    "/api/user/pets": {
      "post": {
        "tags": [
          "User"
        ],
        "summary": "Add your pet",
        "parameters": [],
        "security": [
          {
            "Bearer": []
          }
        ],
        "requestBody": {
          "description": "pet object",
          "required": true,
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/addPetRequest"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Successful added pet",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/addPetResponse"
                }
              }
            }
          },
          "400": {
            "description": "Bad request ",
            "content": {}
          }
        }
      }
    },
