          }
        }
      }
    },
    "/api/users/favoriteNotices/{noticeId}": {
      "patch": {
        "tags": [
          "Notices"
        ],
        "summary": "Add notice to user's favorite",
        "parameters": [
          {
            "in": "path",
            "name": "noticeId",
            "required": true,
            "type": "string",
            "description": "Notice id"
          }
        ],
        "responses": {
          "200": {
            "description": "favorites notices updated",
            "content": {}
          },
          "401": {
            "description": "Not authorized",
            "content": {}
          }
        }
      }
    }
  },
